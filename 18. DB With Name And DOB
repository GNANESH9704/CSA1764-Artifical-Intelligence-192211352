birthdays = [
    ("july", 2, 4, 1999),
    ("may", 20, 5, 1998),
    ("june", 12, 6, 2002),
    ("jan", 7, 9, 2004),
    ("jaap", 8, 6, 2006),
    ("jesus", 9, 7, 2008),
    ("joop", 8, 7, 2001),
    ("jelle", 17, 3, 1995),
]

for name, day, month, year in birthdays:
    print(f"{name} was born on {day}/{month}/{year}")
