students = {
    "john": ["math101"],
    "mary": ["eng202"],
    "ajit": ["cs303"]
}
teachers = {
    "mrs_smith": ["math101", "eng202"],
    "mr_jones": ["cs303"]
}
def find_teacher(student):
    return {t: s for s in students.get(student, []) for t, subs in teachers.items() if s in subs}
print("Teachers for John:", find_teacher("john"))
print("Teachers for Mary:", find_teacher("mary"))
